    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --cache --fix"
        ],
        "*.{js,jsx,css,md}": "prettier --write"
    },
    "eslintConfig": {
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    }
